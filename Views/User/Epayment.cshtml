                                                                   value="@Resources.Resource.Duties"
                                                                   name="payfor"
                                                                   type="text" id="inputduty"
                                                                   readonly="readonly" />
                                                        }
                                                        else
                                                        {
                                                            <input class="form-control" placeholder=""
                                                                   value="@Resources.Resource.Deposits"
                                                                   name="payfor"
                                                                   type="text" id="inputduty"
                                                                   readonly="readonly" />
                                                        }
                                                    }


                                                </div>
                                            </div>
                                        </div>


