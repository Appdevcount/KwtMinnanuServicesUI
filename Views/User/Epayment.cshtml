                                    </fieldset>
                                </div>

                            </td>


                        </tr>


                        <tr>
                            @{
                                if (item.STATUS == "Success" || item.STATUS == "ناجحة" || item.STATUS == "رفض" || item.STATUS == "Denied")


                                {
                                    <td>


                                        <div class="form-group">
                                            @Html.ActionLink(@Resources.Resource.SubEpayment_B_back, "payment", "User", null, new { @class = "btn btn-info btn-block col-centered marginTop50 width50Percent", @style = "width:90%" })
                                        </div>
                                    </td>
                                    <td>
                                        <div class="form-group">
                                            @*@Html.ActionLink(@Resources.Resource.btnprint, "paymentprint", "User", new { @class = "btn button button4" })
                                                <button type="button" style="width:90%" id="btnprint" class="btn btn-info btn-block col-centered marginTop50 width50Percent">@Resources.Resource.btnprint</button>*@
                                            @Html.ActionLink(@Resources.Resource.btnprint, "paymentprint", "User", new { @class = "btn btn-info btn-block col-centered marginTop50 width80Percent" })



                                        </div>
                                    </td>
                                }
                                else
                                {
                                    <td align="center">
                                        <div class="form-group">
                                            <button type="submit" style="width:90%" class="btn btn-info btn-block col-centered marginTop50 width50Percent">@Resources.Resource.SubEpayment_B_pay</button>
                                        </div>
                                    </td>
                                    <td align="center">
                                        <div class="form-group">
                                            @Html.ActionLink(@Resources.Resource.SubEpayment_B_decline, "DenyPayRequest", "User", null, new { @class = "btn btn-info btn-block col-centered marginTop50 width50Percent", @style = "width:90%;background-color: red;" })
