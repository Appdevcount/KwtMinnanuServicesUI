                                                                                <a href="@urll" class="btn btn-info btn-blockdiscard col-centered marginTop15discard width50Percentdiscard" style="text-decoration:none;">@Resources.Resource.editRequest</a>
                                                                            }
                                                                            else
                                                                            {
