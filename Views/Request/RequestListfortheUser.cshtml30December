                                                                                    @Html.ActionLink(Resources.Resource.details, "RequestDetailsfortheRequest", new { EServiceRequestNumber = CommonFunctions.CsUploadEncrypt(@item.EServiceRequestNumber) }, new { @class = "btn btn-info btn-blockdiscard col-centered marginTop15discard width50Percentdiscard", style = "text-decoration:none;" })

                                                                                </b>
                                                                            }
                                                                        </div>
                                                                    </div>
