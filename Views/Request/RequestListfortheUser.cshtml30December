<script type="text/javascript">
    //$(".panel-collapse").click(function () {
    //    var id = this.id;
    //    //   alert(id);
    //});

    var rNumm = '';
    var rNum = '';

    $("#closeMessage, #closeMessage1").click(function ()
    {
        $('#reqUp').val('');
        $('#modalDivForDeleteRequests').css('display', 'none');
    });

    $("#closeMessageOK").click(function () {
        $('#reqUp').val('');
        window.location = window.location;
    });

    $(document).on("click", ".deleteRequest", function () {
        event.preventDefault();

         rNumm = this.id
         rNum = $(this).attr("id");

        //console.log(rNum);
        //console.log(rNumm);

        $('#reqUp').val(rNumm);

        $('#modalDivForDeleteRequests').css('display', 'block');

    });



    $("#deleteConfirmed").click(function ()
    {
        event.preventDefault();

        //console.log(rNum);
        //console.log(rNumm);

        var l = getCookie("culture");

        var deleteSuccess = "تم حذف الطلب بنجاح";
        var deleteFailed = "لم يتم حذف الطلب، الرجاء اعادة المحاولة في وقت لاحق";

        if (l.indexOf('en') != -1 )
        {
            deleteSuccess = "Request Has Been Deleted Successfully";
            deleteFailed = "Request Deletion Failed, Please Try Again Later";
        }

        //return;

         $.ajax({
            type: "POST",
            async: true,
            cache: false, // Added for Security
            url: '@Url.Action("DeleteRequest", "Request")',
            data: JSON.stringify({ 'requestNumber': rNum }),
            contentType: 'application/json; charset=utf-8',
            dataType: 'json',
             error: function (XMLHttpRequest, textStatus, errorThrown)
             {
                alert("Request: " + XMLHttpRequest.toString() + "\n\nStatus: " + textStatus + "\n\nError: " + errorThrown);
            },
             success: function (result)
             {
                if (result)
                {

                    if (result.message == "1")
                    {
                        $('#reqUp').val('');
                        $("#deleteConfirmed").css('display', 'none');
                        $("#closeMessage1").css('display', 'none');
                        $("#closeMessageOK").css('display', 'block');

                        $('#modalMessage').text(deleteSuccess);
                        $('#modalDivForDeleteRequests').css('display', 'block');

                    }
                    else if (result.message == "-1")
                    {
                        $('#reqUp').val('');
                        $("#deleteConfirmed").css('display', 'none');
                        $("#closeMessage1").css('display', 'none');
                        $("#closeMessageOK").css('display', 'block');

                        $('#modalMessage').text(deleteFailed);
                        $('#modalDivForDeleteRequests').css('display', 'block');
                    }
                }
            }// End Success
        });  // End Ajax Request

    });

</script>

<!-- The Modal -->
<div id="modalDiv" class="modal">

    <!-- Modal content -->
    <div class="modal-content">
        <div class="modal-header">
            <span class="close" id="closeError">&times;</span>
            <h6 id="modelHeaderTitle">&nbsp;</h6>
        </div>
        <div class="modal-body">
            <p id="modalMessage">
                @Resources.Resource.DeleteRequest
            </p>
        </div>
        <div class="modal-footer">
            @*<input type="button" ID="closeModal" class="btn btn-info" value="@Resources.Resource.Ok" />*@
            <a id="OrgReqDelete" href="../User/OrgDelete?OrgReqId=" class="btn btn-info btn-blockdiscard col-centered  " style="text-decoration:none;width: 80px;">@Resources.Resource.yes</a>
            @*@Html.ActionLink(Resources.Resource.delete, "OrgDelete", "User", new { OrgReqId = encodedidn }, new { @class = "btn btn-info btn-blockdiscard col-centered marginTop50discard width50Percentdiscard", style = "text-decoration:none;", @onclick = "stop(event)" })*@
        </div>
    </div>

</div>




