                                    <i class="fa fa-arrow-circle-right"></i>
                                </div>*@
                            </div>
                        </div>
                    </a>
                </div>
            </div>
            <div class="col-lg-5">
                <div class="panel panel-success">
                    <div class="panel-heading">
                        <div class="row">
                            <div class="col-xs-6">
                                <i class="fa fa-thumbs-o-up fa-5x"></i>
                            </div>
                            <div class="col-xs-6 text-center">
                                <p class="announcement-heading">@approvedRequests</p>
                                <p class="announcement-text">@Resources.Resource.dashBoardApprovedRequests</p>
