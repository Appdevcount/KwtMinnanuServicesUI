
@{
    ViewBag.Title = Resources.Resource.dashBoard;
    Layout = "~/Views/Shared/_LayoutPageFinal.cshtml";
}


@{

    String allRequests = String.Empty;
    String rejectedRequests = String.Empty;
    String approvedRequests = String.Empty;
    String examRequests = String.Empty;
    String organizationRequests = String.Empty;
    String renewalRequests = String.Empty;
    String submittedRequests = String.Empty;
    String subscriptionRequests = String.Empty;
    String mySerivces = String.Empty;

    System.Data.DataTable dataTable = new System.Data.DataTable();

    if (ViewData.Model != null)
    {

        dataTable = (System.Data.DataTable)ViewData.Model;
    }

    foreach (System.Data.DataRow item in dataTable.Rows)
    {
        allRequests = "(" + item["AllRequests"].ToString() + ")";
        rejectedRequests = "(" + item["AllRejectedRequests"].ToString() + ")";
        approvedRequests = "(" + item["AllApprovedRequests"].ToString() + ")";
        examRequests = "(" +  item["ExamRequests"].ToString() + ")";
        organizationRequests = "(" +  item["OrganizationsRequests"].ToString() + ")";
        renewalRequests = "(" +  item["RenewalRequests"].ToString() + ")";
        submittedRequests = "(" +  item["AllSubmittedRequests"].ToString() + ")";
        subscriptionRequests = "(" +  item["SubscriptionRequests"].ToString() + ")" ;
        mySerivces = "(" + item["YourServices"].ToString() + ")";
    }



    <div class="container marginTop15">
        <div class="row">
            <div class="col-lg-5">
                <div class="panel panel-primary">
                    <div class="panel-heading">
                        <div class="row">
                            <div class="col-xs-6">
                                <i class="fa fa-building-o fa-5x"></i>
                            </div>
                            <div class="col-xs-6 text-center">
                                <p class="announcement-heading">@allRequests</p>
                                <p class="announcement-text">@Resources.Resource.dashBoardAllRequests</p>
                            </div>
                        </div>
                    </div>
                    <a href="#">
                        <div class="panel-footerEservice announcement-bottom">
                            <div class="row">
                                <div class="col-xs-12 text-center">
                                    @Resources.Resource.details
                                </div>
                                @*<div class="col-xs-6 text-center">
                                    <i class="fa fa-arrow-circle-right"></i>
                                </div>*@
                            </div>
                        </div>
                    </a>
                </div>
            </div>
            <div class="col-lg-5">
                <div class="panel panel-info">
                    <div class="panel-heading">
                        <div class="row">
                            <div class="col-xs-6">
                                <i class="fa fa-paper-plane fa-5x"></i>
                            </div>
