            $("#closeModal").click(function () {

                window.location.assign(applicationUrl + 'Request/RequestListfortheUser');
                //window.location.href = applicationUrl + 'Request/RequestListfortheUser';
            });


            function getCookies(cname) {
                var name = cname + "=";
                var decodedCookie = decodeURIComponent(document.cookie);
                var ca = decodedCookie.split(';');
                for (var i = 0; i < ca.length; i++) {
                    var c = ca[i];
                    while (c.charAt(0) == ' ') {
                        c = c.substring(1);
                    }
                    if (c.indexOf(name) == 0) {
                        return c.substring(name.length, c.length);
                    }
                }
                return "";
            }


            var language = getCookies("culture");

            var successHeader = '';
            var successMsg = '';

            var status = '@ViewBag.requestSubmitted';

            //alert(status);

            if (status == 'Submit') {
                successHeader = "رسالة";
                successMsg = "تم ارسال طلبك بنجاح، يمكنك متابعة حالة الطلب من خلال الذهاب الى صفحة طلباتي";


                if (language.indexOf("en") != -1) {
                    successHeader = "Message";
                    successMsg = "Your Request has been Sent, you can follow up status by using My Requests Page";
                }

                $("#modelHeaderTitle").text(successHeader);
                $("#modalMessage").text(successMsg);

                $("#modalDiv").css("display", "block");

            }

            else if (status == 'Draft') {
                successHeader = "رسالة";
                successMsg = "تم حفظ معلومات طلبك بنجاح، يمكنك متابعة و تعديل الطلب من خلال الذهاب الى صفحة طلباتي";


                if (language.indexOf("en") != -1) {
                    successHeader = "Message";
                    successMsg = "Your Request Information has been Saved, you can follow and Edit by using My Requests Page";
                }

                $("#modelHeaderTitle").text(successHeader);
                $("#modalMessage").text(successMsg);

                $("#modalDiv").css("display", "block");
            }

            else if (status == 'Duplicated')
            {
                successHeader = "رسالة";
                successMsg = "يوجد طلب للاشتراك في الاختبار لهذا الرقم المدني، لا يمكن انشاء اكثر من طلب اشتراك اختبار لنفس الرقم المدني";


                if (language.indexOf("en") != -1)
                {
                    successHeader = "Message";
                    successMsg = "You Have already an Existing Exam Request for The Entered Civil Id";
                }

                $("#modelHeaderTitle").text(successHeader);
                $("#modalMessage").text(successMsg);

                $("#modalDiv").css("display", "block");
            }


            //setTimeout(function () {
            //    window.location.href = applicationUrl + 'Request/RequestListfortheUser';
            //}, 400);

        </script>
